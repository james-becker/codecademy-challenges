/*Write a function on_all that applies a function
to every element of a list. Use it to print the
first twenty perfect squares.*/

var onAll = function(howMany) {
  
  var ifSquarePrint = function(number) {
    if (Math.sqrt(number) % 1 === 0) {
      console.log("|" + number + "|");
    }
  };
  
  for ( i = 1 ; i <= howMany ; i++ ) {
    ifSquarePrint(i);
  }
};

console.log(onAll(2000));
